    <string name="sql_tmp_Contact_DropTable">
        DROP TABLE IF EXISTS tmp_Contact;
    </string>

    <!--language=SQLite-->
    <string name="sql_tmp_Contact_CreateTable">
        CREATE TABLE IF NOT EXISTS tmp_Contact(
            ContactId TEXT primary key,
            PhoneContactId TEXT,
            DisplayName TEXT,
            Phones Text,
            Emails Text,
            ImageUri Text,
            UpdatedAt INTEGER
        );
